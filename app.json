{
  "expo": {
    "name": "wordjotter-temp",
    "slug": "wordjotter-temp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/icons/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "NOTIFICATIONS"
        ]
      }
    },
    "web": {
      "favicon": "./src/assets/icons/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icons/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./src/assets/sounds/reminder.wav"
          ]
        }
      ],
      "expo-web-browser"
    ]
  }
}
